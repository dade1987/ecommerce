{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-curator": "^3.0",
        "bezhansalleh/filament-shield": "^3.0@beta",
        "digikraaft/laravel-review-rating": "^3.0",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "filament/filament": "^3.0-stable",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "openai-php/client": "v0.10.0-beta.1",
        "rupadana/filament-api-service": "^1.0",
        "saade/filament-fullcalendar": "3.0",
        "sevendays-digital/filament-nested-resources": "*",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-livewire-wizard": "^2.3",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-tags": "^4.6",
        "thecodingmachine/safe": "^2.5",
        "van-ons/laraberg": "^2.0",
        "z3d0x/filament-fabricator": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.52",
        "larastan/larastan": "^2.9",
        "laravel/breeze": "^1.23",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laraxot/laravel-auto-dev": "dev-master",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "php-parallel-lint/php-code-style": "^2.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpro/grumphp": "^1.16",
        "psalm/plugin-laravel": "^2.11",
        "spatie/laravel-ignition": "^2.0",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "vimeo/psalm": "^5.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/sevendays-digital/filament-nested-resources"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
